        { "source": "hi.aihuazhi.cn/:path*", "destination": "/hi/:path*" },
        { "source": "ko.aihuazhi.cn/:path*", "destination": "/ko/:path*" },
        { "source": "zh-hant.aihuazhi.cn/:path*", "destination": "/zh-Hant/:path*" },
        { "source": "tr.aihuazhi.cn/:path*", "destination": "/tr/:path*" },
        { "source": "aihuazhi.cn/:path*", "destination": "/en/:path*" }
