{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "installCommand": "npm install",
    "framework": "astro",
    "rewrites": [
        { "source": "/:path*", "destination": "/zh-CN/:path*", "has": [{ "type": "host", "value": "zh.*" }] },
        { "source": "/:path*", "destination": "/zh-Hant/:path*", "has": [{ "type": "host", "value": "zh-hant.*" }] },
        { "source": "/:path*", "destination": "/de/:path*", "has": [{ "type": "host", "value": "de.*" }] },
        { "source": "/:path*", "destination": "/ja/:path*", "has": [{ "type": "host", "value": "ja.*" }] },
        { "source": "/:path*", "destination": "/fr/:path*", "has": [{ "type": "host", "value": "fr.*" }] },
        { "source": "/:path*", "destination": "/ar/:path*", "has": [{ "type": "host", "value": "ar.*" }] },
        { "source": "/:path*", "destination": "/es/:path*", "has": [{ "type": "host", "value": "es.*" }] },
        { "source": "/:path*", "destination": "/it/:path*", "has": [{ "type": "host", "value": "it.*" }] },
        { "source": "/:path*", "destination": "/pt-pt/:path*", "has": [{ "type": "host", "value": "pt.*" }] },
        { "source": "/:path*", "destination": "/nl/:path*", "has": [{ "type": "host", "value": "nl.*" }] },
        { "source": "/:path*", "destination": "/pl/:path*", "has": [{ "type": "host", "value": "pl.*" }] },
        { "source": "/:path*", "destination": "/ru/:path*", "has": [{ "type": "host", "value": "ru.*" }] },
        { "source": "/:path*", "destination": "/th/:path*", "has": [{ "type": "host", "value": "th.*" }] },
        { "source": "/:path*", "destination": "/id/:path*", "has": [{ "type": "host", "value": "id.*" }] },
        { "source": "/:path*", "destination": "/vi/:path*", "has": [{ "type": "host", "value": "vi.*" }] },
        { "source": "/:path*", "destination": "/ms/:path*", "has": [{ "type": "host", "value": "ms.*" }] },
        { "source": "/:path*", "destination": "/ml/:path*", "has": [{ "type": "host", "value": "ml.*" }] },
        { "source": "/:path*", "destination": "/my/:path*", "has": [{ "type": "host", "value": "my.*" }] },
        { "source": "/:path*", "destination": "/hi/:path*", "has": [{ "type": "host", "value": "hi.*" }] },
        { "source": "/:path*", "destination": "/ko/:path*", "has": [{ "type": "host", "value": "ko.*" }] },
        { "source": "/:path*", "destination": "/tr/:path*", "has": [{ "type": "host", "value": "tr.*" }] }
    ]
}