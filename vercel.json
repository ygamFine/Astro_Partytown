{
    "version": 2,
    "buildCommand": "astro build",
    "outputDirectory": "dist",
    "installCommand": "npm install",
    "framework": "astro",
    "rewrites": [
        { "source": "en.example.com/:path*", "destination": "/en/:path*" },
        { "source": "ja.example.com/:path*", "destination": "/ja/:path*" },
        { "source": "de.example.com/:path*", "destination": "/de/:path*" },
        { "source": "/:path*", "destination": "/en/:path*" }
      ],
  }