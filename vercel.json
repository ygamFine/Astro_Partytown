        { "source": "zh.aihuazhi.cn/:path*", "destination": "/zh-CN/:path*" },
        { "source": "de.aihuazhi.cn/:path*", "destination": "/de/:path*" },
        { "source": "ja.aihuazhi.cn/:path*", "destination": "/ja/:path*" },
        { "source": "fr.aihuazhi.cn/:path*", "destination": "/fr/:path*" },
        { "source": "ar.aihuazhi.cn/:path*", "destination": "/ar/:path*" },
        { "source": "es.aihuazhi.cn/:path*", "destination": "/es/:path*" },
        { "source": "it.aihuazhi.cn/:path*", "destination": "/it/:path*" },
        { "source": "pt.aihuazhi.cn/:path*", "destination": "/pt-pt/:path*" },
        { "source": "nl.aihuazhi.cn/:path*", "destination": "/nl/:path*" },
        { "source": "pl.aihuazhi.cn/:path*", "destination": "/pl/:path*" },
        { "source": "ru.aihuazhi.cn/:path*", "destination": "/ru/:path*" },
        { "source": "th.aihuazhi.cn/:path*", "destination": "/th/:path*" },
